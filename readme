docker build -t nginx .

docker run --name nginx --rm -d -p 80:80 nginx

docker run --name nginx --rm -it -p 80:80 nginx (#CMD)

docker run --name nginx --rm -it -p 80:80 --link wordpress nginx

docker build -t wordpress .

docker run --name wordpress --rm -d -p 80:80 wordpress

docker run --name wordpress --rm -it -p 80:80 wordpress (#CMD)
